{
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "target": "ES5",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "lib": ["dom", "dom.iterable", "ES2021"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
